{
  "name": "glue-ui-vue",
  "version": "0.0.4",
  "main": "dist/glue-ui-vue.common.js",
  "license": "MIT",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name glue-ui-vue ./src/lib-entry.js",
